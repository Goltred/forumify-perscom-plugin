{% extends '@Forumify/frontend/base.html.twig' %}
{% block title_page %}{{ perscomForm.name }}{% endblock %}
{% block body %}
    <h1 class="mb-6">{{ perscomForm.name }}</h1>
    <div class="box">
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <div class="flex justify-center gap-4">
            <button class="btn-primary">{{ 'save'|trans }}</button>
            <a href="{{ path('perscom_operations_center') }}" class="btn-outlined">{{ 'cancel'|trans }}</a>
        </div>
        {{ form_end(form) }}
    </div>

{% endblock %}
