{
    "name": "forumify/forumify-perscom-plugin",
    "type": "symfony-bundle",
    "license": "OSL-3.0",
    "version": "0.1.0",
    "require": {
        "php": ">=8.2",
        "forumify/forumify-platform": "*",
        "deschutesdesigngroupllc/perscom-php-sdk": "dev-master"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../forumify-platform"
        }
    ],
    "autoload": {
        "psr-4": {
            "Forumify\\PerscomPlugin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false
        }
    }
}
