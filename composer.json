{
    "name": "forumify/forumify-perscom-plugin",
    "type": "forumify-plugin",
    "keywords": ["symfony-ux"],
    "license": "OSL-3.0",
    "require": {
        "php": ">=8.2",
        "forumify/forumify-platform": "*",
        "deschutesdesigngroupllc/perscom-php-sdk": "^1.0.12"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../forumify-platform"
        }
    ],
    "autoload": {
        "psr-4": {
            "Forumify\\PerscomPlugin\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false
        }
    },
    "extra": {
        "forumify-plugin-class": "Forumify\\PerscomPlugin\\ForumifyPerscomPlugin"
    }
}
